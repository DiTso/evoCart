<div class="sectionBody">
    <div id="modulePane" class="dynamic-tab-pane-control tab-pane">
        <div class="tab-row">
            <h2 class="tab selected"><span>Список</span></h2>
        </div>
        <div id="tab-page1" class="tab-page" style="display:block;">
            <table class="table data table-striped table-hover">
                <col width="20" valign="top">

                <thead>
                    <tr>
                        <th width="20">#</th>
                        <th>Дата</th>
                        <th>Доставка</th>
                        <th>Заказчик</th>
                        <th></th>
                        <th>Сумма</th>
                        <th></th>
                        <th>Статус</th>
                    </tr>
                </thead>
                <tbody>
                    [+list+]
                </tbody>
                <tfoot>
                    <tr>
                        <td colspan="5">
                            [+list.pages+]
                        </td>
                    </tr>
                </tfoot>
    		</table>
        </div>
    </div>
</div>
